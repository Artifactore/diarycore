generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model reminder {
  id          Int      @id @default(autoincrement())
  chat_id     BigInt
  text        String
  trigger_at  DateTime
  sent        Boolean  @default(false)
  created_at  DateTime @default(now())
}
